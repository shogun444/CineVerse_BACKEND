generator client {
  provider = "prisma-client-js"
   output   = "../node_modules/@prisma/movies-client"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Videos{
  id Int @id @default(autoincrement())
  file_id String @unique
  popularity String
  file_name String 
  genre  String[] @default([])
  language String 
  file_size  String? @default("")
  thumbnail  String?
  backdrop String?
  releaseDate  String?
  message_id Int?
  chat_id String     
  telegram_link  String?
  rating String?
  title String
  tmdb_id       Int?
}

